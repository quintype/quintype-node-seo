    "standard-version": "^8.0.1",
    "url": "^0.11.0"
  },
  "dependencies": {
    "date-fns": "^2.17.0",
    "date-fns-tz": "^1.1.1",
    "quintype-js": "^1.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npx eslint --fix",
      "git add"
    ]
  }
